<h1><%="#{@playlist.user.name}'s Playlist: #{@playlist.name}"%></h1>
<table>
  <tr>
    <th>ID</th>
    <th>Song Name</th>
    <th>Artist</th>
  </tr>
  <% @playlist.songs.each do |song| %>
    <tr>
      <td><%= song.id %></td>
      <td><%= song.name %></td>
      <td><%= link_to song.artist.name, artist_path(song.artist) %></td>
    </tr>
  <% end %>
</table>
